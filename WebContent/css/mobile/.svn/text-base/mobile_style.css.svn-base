#wrap{padding:0px; margin:0px; width:100%; margin-left:auto; margin-right:auto; text-align:left; border:0px solid #037ed4;}
#header  {margin:0 auto;}
#content {margin:0 auto; padding-left:5px; padding-right:5px; min-height:540px; }
#content2 {margin:0 auto; }
#footer  { background-color:#e3e3e3;  margin-top:50px; }

/* 상단 로고 */
.top_bg {position:relative; background-color:#b8bccc; height:45px; padding:13px 0 0 0; text-align:center;}
.top_bg .allmenu {position:absolute; right:10px; top:6px; display:block; background-color:#fff; width:30px; height:30px; background:url(/images/mobile/allmenu.png) no-repeat center center; font-size:0px; }
.top_bg .logon {position:absolute; right:40px; top:9px; display:block; width:25px; height:25px; background:url(/images/mobile/logon.png) no-repeat center center; font-size:0px;}
.top_bg .logout {position:absolute; right:40px; top:9px; display:block; width:25px; height:25px; background:url(/images/mobile/logout.png) no-repeat center center; font-size:0px;}
.top_bg .logo {position:absolute; left:10px; display:block; width:250px; height:25px; background:url(/images/mobile/logo.png) no-repeat center center; font-size:0px;}



/*전체메뉴*/
.navi_con {position:absolute; left:0px; top:0px;  background-color:#fff; width:100%; z-index:990;}
.navi_con .top {background-color:#b8bccc; height:200px;}
.navi_con .top p {position:relative; color:#fff; text-align:left; padding:10px 15px;  border-bottom:1px solid #b2b5bf; font-size:1.1em;}
.navi_con .top p .close_bt {position:absolute; right:0px; top:12px; color:#fff; width:30px; height:30px;    background:url(/images/mobile/bu4.png) no-repeat 0px 0px; font-size:0px;}
.navi_con .top .member_con {display:table; padding:10px 20px; width:100%;}
.navi_con .top .member_con li {display:table-cell; color:#fff; text-align:center; font-weight:700;}
.navi_con .top .member_con li a {font-size:0px;}
.navi_con .top .member_con li .home {display:block; width:70px; height:70px; border:1px solid #fff; background:url(/images/mobile/home_img.png) no-repeat center center #b8bccc;   border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .login {display:block; width:70px; height:70px; border:1px solid #fff; background:url(/images/mobile/login_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .member {display:block; width:70px; height:70px; border:1px solid #fff; background:url(/images/mobile/member_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .idpw_find {display:block; width:70px; height:70px;  border:1px solid #fff; background:url(/images/mobile/search_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .logoff {display:block; width:70px; height:70px; border:1px solid #fff; background:url(/images/mobile/logoff_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .privacy {display:block; width:70px; height:70px; border:1px solid #fff; background:url(/images/mobile/privacy_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}
.navi_con .top .member_con li .pwmodi {display:block; width:70px; height:70px;  border:1px solid #fff; background:url(/images/mobile/pwmodi_img.png) no-repeat center center #b8bccc;    border-radius:50%; margin:10px auto;}


.navi_con .navi {margin:0px 0 40px 0; }
.navi_con .navi li {display:block; text-align:left; border-bottom:1px solid #ddd; padding-bottom:10px; }
.navi_con .navi li a {display:block; font-weight:700; font-size:1.1em; background:url(/images/mobile/bu2.png) no-repeat 27px 15px; padding:10px 0 10px 37px;}
.navi_con .navi li .text_on {color:#fff; background:url(/images/mobile/bu1.png) no-repeat 25px 15px #4da5ff;}

.navi_con .navi li:nth-child(4), .navi_con .navi li:nth-child(5) {padding-bottom:0px;  }
.navi_con .navi .depth2 {padding:10px 0 0 0px; display:block; border-top:1px solid #ddd; }
.navi_con .navi .depth2  a {display:block; font-weight:400; background:url() no-repeat 40px center;  padding:5px 0px 0px 50px;  height:30px;  }
.navi_con .navi .depth2  a:hover {text-decoration:underline;}

.navi_con .navi .depth3  a {display:block; font-weight:400;  background:url(/images/mobile/bu3.png) no-repeat 40px center; margin-left:15px;  padding:5px 0px 0px 50px;  height:30px; }
.navi_con .navi .depth3  a:hover {text-decoration:underline;}


/*.top_bg .search  {position:absolute; right:10px; top:6px; display:block; background-color:#fff; width:30px; height:30px; background:url(/images/mobile/search.png) no-repeat center center; font-size:0px;}*/


.slogan {color:#aedbff; font-size:1.8em; height:150px; margin:0 auto; text-align:center;  padding-top:30px; line-height:35px;}
.slogan span {color: #f8ffff; font-size:1em; font-weight:700;}

/*메뉴 바로가기*/
.gnb {width:100%; margin:0 auto;}
.gnb .menu1 {display:table; width:100%; margin-top: 50px;}
.gnb .menu1 li {position:relative;  display:table-cell; width:23%; height:100px; padding-top:70px; font-weight:700;  text-align:center; }
.gnb .menu1 li a {position:absolute; left:0px; top:0px; width:100%; height:100%; font-size:0px;}
.gnb .menu1 li:nth-child(1) {background:url(/images/mobile/menu_img1.png) no-repeat center 3px;} /*공지사항*/
.gnb .menu1 li:nth-child(2) {background:url(/images/mobile/menu_img2.png) no-repeat center 0px;} /*사업안내*/
.gnb .menu1 li:nth-child(3) {background:url(/images/mobile/menu_img3.png) no-repeat center 3px;} /*사업공고*/
.gnb .menu1 li:nth-child(4) {background:url(/images/mobile/menu_img4.png) no-repeat center 3px; width:26%; } /*추진일정예보*/

.gnb .menu2 {display:table; border-collapse:collapse; width:100%; margin-top:10px;  border-left:5px solid #fff; border-right:5px solid #fff;}
.gnb .menu2 li {position:relative;  display:table-cell; width:31%; height:110px; font-weight:700; padding:10px 0 0 10px; font-size:1.2em; color:#fff; line-height:22px; border:7px solid #fff;}
.gnb .menu2 li a {position:absolute; left:0px; top:0px; width:100%; height:100%; font-size:0px; }
.gnb .menu2 li.none {width:2px; background-color:none; background:none; }
.gnb .menu2 li:nth-child(1) {background:url(/images/mobile/menu_img6S.png) no-repeat 90% 85%; background-color:#2e8bf4;}  /*과제현황조회*/
.gnb .menu2 li:nth-child(2) {background:url(/images/mobile/menu_img5S.png) no-repeat 90% 85%; background-color:#18a6e1;} /*연구비집행현황조회*/
.gnb .menu2 li:nth-child(3) {background:url(/images/mobile/menu_img8S.png) no-repeat 90% 85%; background-color:#1ebcd8;} /*납부계획조회*/
.gnb .menu2 li.logOut {background:url(/images/mobile/menu_img501.png) no-repeat 90% 85%; background-color:#2e8bf4;}


.gnb .menu3 {display:table;  border-collapse:collapse; width:100%; border-left:5px solid #fff; border-right:5px solid #fff;}
.gnb .menu3 li {position:relative; display:table-cell; width:31%; height:110px; font-weight:700; padding:10px 0 0 10px; font-size:1.2em; color:#fff; line-height:22px; border:7px solid #fff; border-top:5px; }
.gnb .menu3 li a {position:absolute; left:0px; top:0px; width:100%; height:100%; font-size:0px;}
.gnb .menu3 li:nth-child(1) {background:url(/images/mobile/menu_img7S.png) no-repeat 90% 85%; background-color:#0dcaad;} /*전자공지함*/
.gnb .menu3 li:nth-child(2) {background:url(/images/mobile/menu_img9S.png) no-repeat 90% 85%; background-color:#a5c71e;} /*규정조회*/
.gnb .menu3 li:nth-child(3) {background:url(/images/mobile/menu_img10S.png) no-repeat 90% 85%; background-color:#f1aa20;} /*평가위원참여*/


/*사업공고*/
.busin_notice {margin-top:40px;}
.busin_notice h3 {position:relative; font-size:1.7em; color:#333; font-weight:700; height:35px; padding-left:10px;}
.busin_notice h3 a {position:absolute; right:15px; top:-10px; display:block; width:30px; height:30px; background:url(/images/mobile/more.gif) no-repeat center center; border:1px solid #cecece; font-size:0px; }

.busin_notice .notice_bg {background:url(/images/mobile/notice_bg.jpg) no-repeat center 0px;  background-size:cover; padding:20px 20px; min-height:100px;}
.busin_notice .notice_bg .noticeNot {/*background-color:#6593df; filter:alpha(opacity=70); opacity:0.7; */ height:100px;  text-align:center;  padding-top:50px;}
.busin_notice .notice_bg .noticeNot span {color:#23406a; font-weight:700;  font-size:1.15em;} 
.busin_notice .notice_bg ul {display:table; width:100%; margin-top:15px;}
.busin_notice .notice_bg li.texton {background-color:#429af5; color:#fff;}
.busin_notice .notice_bg li {position:relative; display:block; height:80px; background-color:#fff; font-weight:700; font-size:1.1em; padding:10px 10px;  margin-bottom:10px;   line-height:22px;  word-break:break-word; vertical-align:top; border:5px solid transparent; /*transparent*/}
.busin_notice .notice_bg li.none {width:1%; background:none; padding:0px 0px; display:block;}
.busin_notice .notice_bg li a {position:absolute; top:0px; left:0px; width:100%; height:100%; font-size:0px;}
.busin_notice .notice_bg li .more_icon {position:absolute; bottom:10%; left:40%; border-radius:50%; width:30px; height:30px; text-align:center; font-weight:700; font-size:1.5em;  background-color:#fff;  color:#116cd8; padding-top:6px;  }
.busin_notice .notice_bg .more_bt {display:block; margin:0 auto; color:#fff; font-weight:700; width:70%; height:35px; padding-top:8px; text-align:center; margin-top:30px; background:url(/images/mobile/more2.png) no-repeat 80% center #8ba0b6;}


/*공지사항*/
.noticeList   {margin-top:40px;}
.noticeList h3 {position:relative; font-size:1.7em; color:#333; font-weight:700; height:35px; padding-left:10px; border-bottom:1px solid #ddd; margin-bottom:15px;}
.noticeList h3 a {position:absolute; right:10px; top:-10px; display:block; width:30px; height:30px; background:url(/images/mobile/more.gif) no-repeat center center; border:1px solid #cecece;  font-size:0px;}
.noticeList ul {}
.noticeList li {position:relative;  height:35px; padding-left:10px; }
.noticeList li a {font-weight:700; display:block; width:65%; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.noticeList li span {position:absolute; right:10px; top:0px; font-weight:400;}

/*고객센터*/
.customer {margin-top:0px;}
.customer h3 {position:relative; font-size:1.7em; color:#333; font-weight:700; height:35px; padding-left:10px;}
.customer h3 a {position:absolute; right:10px; top:-10px; display:block; width:30px; height:30px; background:url(/images/mobile/more.gif) no-repeat center center; border:1px solid #cecece;  font-size:0px;}
.customer .customer_bg {padding:10px 10px 0px 10px; background:url(/brg/images/mobile/mobileMainBanner.jpg) no-repeat center center #edeff7; background-size:cover;}
.customer .customer_bg2 {padding:100% 0px 0px 0px; background:url(/brg/images/mobile/mobile_resinfo.png) no-repeat center center #edeff7; background-size:100% 100%;}
.customer .customer_bg .main_tab {display:table; width:100%;}
.customer .customer_bg .main_tab li {position:relative; display:table-cell; width:50%;  border:1px solid #abc0dc; height:40px; font-weight:700; border-right:1px solid #abc0dc; text-align:center; padding-top:10px; background-color:#fff;}
.customer .customer_bg .main_tab a {position:absolute; top:0px; left:0px; width:100%; height:100%; font-size:0px;}
.customer .customer_bg .main_tab li.tab_on {background-color:#3d81de; color:#fff; border:none;}

.customer .faq_more {background-color:#edeff7; padding:10px 10px 50px 10px;}
.customer .faq_more ul {display:table; width:100%; margin-top:5px;}
.customer .faq_more li {position:relative; display:table-cell;  width:49.5%; border:1px solid #ccc; height:37px; font-weight:700; border-right:1px solid #ccc; padding:8px 0 0px 10px;  background:url(/images/mobile/arr.gif) no-repeat 95% center #fff;  margin:0px 0px 0px 12px;}
.customer .faq_more li.none {width:1%; border:none; background-color:none; background:none;  font-size:0px; }
.customer .faq_more li a {position:absolute; top:0px; left:0px; width:100%; height:100%; font-size:0px;  }
.customer .faq_more li:nth-child(odd) {margin-left:0px;}
.customer .faq_more .half {width:48.7%;}


/*카피라이터*/
.etc {display:table; width:100%; background-color:#8e92a2;}
.etc li {position:relative;  display:table-cell;  height:35px; padding-top:8px; text-align:center; color:#fff; background:url(/images/mobile/line.gif) no-repeat right center; font-size:0.9em;    }
.etc li:last-child {background:none; width:18%;} 
.etc li a {position:absolute; top:0px; left:0px; width:100%; height:100%; font-size:0px;}
/* .call {background:url(/images/mobile/cell_icon.png) no-repeat 18px 15px; font-size:1.3em; color:#444; font-weight:700; padding:17px 0 0 60px; height:40px;} */
.call {background: url(/images/mobile/cell_icon.png) no-repeat 0px 3px; font-size: 1.3em; color: #444; font-weight: 700; padding: 0px 0 0 26px; height: 22px; background-size: 22px;}
/* .call span {font-size:1.5em; font-weight:600;} */
.call span {font-size:12px; font-weight:800;}
.add {margin:5px 0 0px 0; padding-bottom:25px;}
.add li {display:block; background-color:#e3e3e3; padding:0px 15px; padding-top:3px; font-size:0.9em; word-break:break-all; }


/*서브 공통 CSS */

.location {width:100%;}
.location .route {display:block; width:100%; border-bottom:1px solid #ccc; height:40px; padding:12px 0 0 0px;  font-weight:700; font-size:1.2em;}
.location .route2 {display:block; width:100%; border-bottom:1px solid #ccc; height:40px; padding:12px 0 0 10px;  font-weight:700; font-size:1.2em;}

.location .route span, .location .route2 span {font-size:1em;}
.location .spot {display:block; text-align:center; height:120px;  font-size:1.7em; color:#fff;  font-weight:700;  padding-top:30px; background:url(/images/mobile/customer_bg6.jpg) no-repeat center 0px; background-size:cover;  }
.location .spot span{display:block; font-weight:400; font-size:0.7em; padding-top:15px; color:#dce2e5;  }

.location .login_spot {display:block; text-align:center; height:120px; font-weight:700; font-size:1.7em; color:#333;  padding-top:30px; background:url(/images/mobile/login_bg.jpg) no-repeat center -50px; background-size:cover;  }
.location .login_spot span{display:block; font-weight:400; font-size:0.7em; padding-top:10px; line-height:22px;}





/*로그인 */
.login_con {padding:30px 20px 0px  20px;}
.login_con .login {width:100%; margin-top:0px; }
.login_con .login li {position:relative; display:block; width:100%; padding-top:7px; font-weight:700;}
.login_con .login li .id {width:100%; height:40px; border:1px solid #ccc; padding-left:10px;  }
.login_con .login li .pw {width:100%; height:40px; border:1px solid #ccc; padding-left:10px;  }
.login_con .login li .find {width:100%; height:40px; border:1px solid #ccc; padding-left:10px; margin:5px 0 10px 0;  }

.login_con .login li a {background-color:#798393; color:#fff; font-weight:700; display:block; width:100%; height:40px; text-align:center; padding-top:10px; font-size:1.2em;   }
.login_con .login li .idpw_text {position:absolute; left:10px; top:17px; color:#777;}
.login_con .login li .idpw_text2 {position:absolute; left:10px; top:41px; color:#777; overflow:hidden; word-break:break-all; white-space:nowrap; width:90%;}
.login_con .find_text {display:block; padding:30px 0 0 7px; background:url(/images/mobile/bu5.png) no-repeat 0px 37px;  }
.login_con .guide_text {display:block; padding:10px 0 0 7px; background:url(/images/mobile/bu5.png) no-repeat 0px 19px;  }
.login_con .text_ck {display:block; font-weight:700; color:#e33107; margin-top:15px;   }

.login_con .idpw_bt {display:table; width:100%; padding:10px 0px 0px 0;}
.login_con .idpw_bt li  {display:table-cell; border:1px solid #ccc; width:50%;}
.login_con .idpw_bt li a {display:block; height:35px; text-align:center; padding-top:7px; font-weight:700;}
.login_con .idpw_bt li:nth-child(2) {width:2%; border:none; }
.login_con .idpw_bt li:last-child {margin-right:0px;} 

/*로그인 탭*/
.login_tab {display:table; width:100%;}
.login_tab li {display:table-cell; border-top:1px solid #ccc;  border-bottom:1px solid #ccc; width:50%;}
.login_tab li.tab_on {background-color:#1d78d6; color:#fff; border:none;}
.login_tab li a {display:block; height:40px; text-align:center; padding-top:10px; font-weight:700;}
.login_tab li .text_on {background-color:#1d78d6; color:#fff;}
.login_tab li:last-child {border-right:1px solid #ccc;}


.title h3 {position:relative;  display:block; font-size:1.3em; color:#333; font-weight:700; padding:30px 0 0 0px;}
.title h3 .more {position:absolute; right:10px; top:25px; display:block; width:30px; height:30px; background:url(/images/mobile/more.gif) no-repeat center center; border:1px solid #cecece;  font-size:0px;}
.title h3 .write_bt {position:absolute; right:10px; top:25px; display:block; border:1px solid #88a2be; border-radius:3px 3px;  color:#055bb3;  font-weight:700; text-align:center; height:30px; padding:3px 10px 0 10px;}

.title2 h3 {position:relative;  display:block; font-size:1.3em; color:#333; font-weight:700; padding:30px 0 0 10px;}
.title2 h3 .more {position:absolute; right:10px; top:25px; display:block; width:30px; height:30px; background:url(/images/mobile/more.gif) no-repeat center center; border:1px solid #cecece;  font-size:0px;}

/*
    display 속성 중 table 관련속성
    table > table
    table-caption > caption
    table-cell > td
    table-column > col
    table-column-group > colgroup
    table-footer-gioup > tfoot
    table-header-group > thead
    table-row > tr
    table-row-group > tbody

*/


