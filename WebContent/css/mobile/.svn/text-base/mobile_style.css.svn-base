@font-face {
	font-family: 'Nanum Gothic';
	font-style: normal;
	font-weight: 400;
	src: local(※), url(/css/mobile/font/NanumGothic-Regular.eot);
	src: local(※), url(/css/mobile/font/NanumGothic-Regular-iefix.eot) format('embedded-opentype'),  url(/css/mobile/font/NanumGothic-Regular.woff2) format('woff2'),  url(/css/mobile/font/NanumGothic-Regular.woff) format('woff'),  url(/css/mobile/font/NanumGothic-Regular.ttf) format('truetype');
}
@font-face {
	font-family: 'Nanum Gothic';
	font-style: normal;
	font-weight: 700;
	src: local(※), url(/css/mobile/font/NanumGothic-Bold.eot);
	src: local(※), url(/css/mobile/font/NanumGothic-Bold-iefix.eot) format('embedded-opentype'),  url(/css/mobile/font/NanumGothic-Bold.woff2) format('woff2'),  url(/css/mobile/font/NanumGothic-Bold.woff) format('woff'),  url(/css/mobile/font/NanumGothic-Bold.ttf) format('truetype');
}
@font-face {
	font-family: 'Nanum Gothic';
	font-style: normal;
	font-weight: 800;
	src: local(※), url(/css/mobile/font/NanumGothic-ExtraBold.eot);
	src: local(※), url(/css/mobile/font/NanumGothic-ExtraBold-iefix.eot) format('embedded-opentype'),  url(/css/mobile/font/NanumGothic-ExtraBold.woff2) format('woff2'),  url(/css/mobile/font/NanumGothic-ExtraBold.woff) format('woff'),  url(/css/mobile/font/NanumGothic-ExtraBold.ttf) format('truetype');
}

@font-face {
	font-family: 'NotoSans-bold';
	font-style: normal;
	font-weight: 800;
	src: local(※), url(/css/mobile/font/NotoSansKR-Bold.otf);
}

@font-face {
	font-family: 'NotoSans-regular';
	font-style: normal;
	font-weight: 800;
	src: local(※), url(/css/mobile/font/NotoSansKR-Regular.otf);
}

.slogan{
	font-family: 'NotoSans-bold', sans-serif;
	text-shadow: 0 0 black;
}

.slogan span{
	font-family: 'NotoSans-bold', sans-serif;
	text-shadow: 0 0 white;
}

body, th, td, select, textarea, button, div, input, p, ol, ul, li, img, a, form, span, iframe, dl, dt, dd {
	font-family: "Nanum Gothic", "NanumGothicBold", "Malgun Gothic", "sans-serif", Helvetica;
	font-weight: 400;
	color: #565656;
	font-size: 14px;
	word-break: keep-all;
	letter-spacing: 0px;
    line-height:20px;
    text-decoration:none;
}

input[type=text] {width:100%; height:30px; border:1px solid #aaa; padding-left:10px;  }

/***브라우져별 라운드 처리 방지**/
input[type=text] {-webkit-appearance:none;-moz-appearance:none;appearance:none;border-radius:0px;}
input[type=password] {-webkit-appearance:none;-moz-appearance:none;appearance:none;border-radius:0px;}
select {-webkit-appearance:none;-moz-appearance:none;appearance:auto;border-radius:0px;}


