@charset "utf-8";   /*서버 CSS*/

.t03 {padding-top:3px;}
.t10 {padding-top:10px;}
.b10 {padding-bottom:10px;}

h3 { font-size:1.7em; color:#333; font-weight:700; height:35px; padding-left:10px;}

.txt_list li  {display :block; padding:0 0 5px 0px; line-height:24px;}

.txtbu_list li  {background:url(../images/common/bu06.gif) no-repeat 0 5px; padding:0 0 3px 11px; display:block; line-height:19px;}

.tdfix {display:block; width:100%;}


.tbl_type06  { border-top:#999 solid 1px;  border-left:#c6cfd7 solid 1px; border-collapse:collapse; width:100%;}
	.tbl_type06 th {
		background-color:#f7f7f7;
		color:#555;
		border-right:#c6cfd7 solid 1px;
		border-bottom:#c6cfd7 solid 1px;
		padding:5px 7px 3px 7px;
		text-align:center;
		font-weight:700;
		height:35px;
		line-height:20px;
	}
	.tbl_type06 th.b {
		background-color:#f7f7f7;
		color:#555;
		border-right:#c6cfd7 solid 1px;
		border-bottom:#c6cfd7 solid 1px;
		padding:5px 5px 3px 15px;
		text-align:center;
		font-weight:400;
		line-height:16px;
	}
	.tbl_type06 th.row {border-top:0}
	.tbl_type06 td {
		border-right:#c6cfd7 solid 1px;
		border-bottom:#c6cfd7 solid 1px;
		padding:5px 7px 3px 7px;
		text-align:left;
		height:35px;
		line-height:18px;
        word-break:break-all;
	}

	.tbl_type06 th.al, .tbl_type06 td.al {text-align:left} .tbl_type06 td.ac {text-align:center} .tbl_type06 td.ar {text-align:right}
	.tbl_type06 th.ll, .tbl_type06 td.ll {border-right:0} 
	.tbl_type06 td.l20 {line-height:14px;} 
	.tbl_type06 td.bb  {/*color:#008bd3; */ font-weight:bold;}
	.tbl_type06 th.std {background-color:#f8f8f8; color:#666;font-weight:bold;  height:20px;}
	.tbl_type06 td.money {text-align:right; padding-right:450px;}
	.tbl_type06 td.money1 {text-align:right; padding-right:412px;}
	.l20{padding-left:20px;}
    .tbl_type06 tr td p{word-break:normal;}

    .tbl_type06 td input {vertical-align:middle;}
    .tbl_type06 td:last-child, .tbl_type06 th:last-child {border-right:none;}