.tbl_type01  {border-top:#96b9dc solid 0px;} /*알립니다 아래 파란라인*/
	.tbl_type01 th {
		background-color:#ffffff;
		color:#505050;
		border-right:#96b9dc solid 0px;
		border-bottom:#2888d4 solid 3px;
		padding:5px 5px 3px 30px;
		text-align:left;
		font-weight:bold;
		font-size:16px;
		line-height:20px;
		height:33px;
	}
	.tbl_type01 td.bott {
		background-color:#ffffff;
		border-right:#96b9dc solid 0px;
		border-bottom:#f2f2f2 solid 2px;
		padding:5px 5px 3px 20px;
		text-align:left;
		font-weight:normal;
		line-height:18px;
		padding-left:20px;
		font-size:14px;
		height:24px;
		font-weight:bold;
		background:url(../../images/mobile/dot02.gif) no-repeat 8px 12px;
	}
	.tbl_type01 th.row {border-top:0}
	.tbl_type01 td {
		border-right:#96b9dc solid 0px;
		border-bottom:#dddddd dotted 1px;
		padding:5px 5px 3px 20px;
		text-align:left;
		line-height:18px;
		font-size:14px;
		height:24px;
		background:url(../../images/mobile/dot02.gif) no-repeat 8px 12px;
	}
	.tbl_type01 th.al, .tbl_type01 td.al {text-align:left}
	.tbl_type01 th.ll, .tbl_type01 td.ll {border-right:0}
	.tbl_line {border-left:#96b9dc solid 1px;}


